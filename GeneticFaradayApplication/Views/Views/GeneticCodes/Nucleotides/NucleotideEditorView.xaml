<Window x:Class="GeneticFaradayApplication.Views.Views.GeneticCodes.Nucleotides.NucleotideEditorView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NucleotideEditorView" Height="125" Width="219">
    <Grid>
        <TextBox HorizontalAlignment="Left" x:Name="nucleotide_identifier_text_box" Height="23" Margin="10,31,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="191"/>
        <Label Content="Nucleotide Identifier" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.158,-0.423" Margin="10,0,0,0">
            <Label.Effect>
                <DropShadowEffect BlurRadius="7" Opacity="0.9"/>
            </Label.Effect>
        </Label>
        <Button Content="Save" x:Name="save_button" HorizontalAlignment="Left" Margin="101,59,0,0" VerticalAlignment="Top" Width="100" Click="save_button_Click_1"/>

    </Grid>
</Window>
